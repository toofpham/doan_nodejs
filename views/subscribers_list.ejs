<!DOCTYPE html>
<html>
<head>
  <title>Danh sách Subscribe</title>
  <link rel="stylesheet" href="/css/bootstrap.min.css">
</head>
<body>
<div class="container mt-5">
  <h1>Danh sách Subscribe</h1>
  <table class="table table-bordered">
    <thead>
      <tr>
        <th>ID</th>
        <th>Email</th>
        <th>Ngày đăng ký</th>
      </tr>
    </thead>
    <tbody>
      <% subscribers.forEach(subscriber => { %>
        <tr>
          <td><%= subscriber.id %></td>
          <td><%= subscriber.email %></td>
          <td><%= new Date(subscriber.created_at).toLocaleString('vi-VN') %></td>
        </tr>
      <% }); %>
    </tbody>
  </table>
</div>
<script src="/js/bootstrap.min.js"></script>
</body>
</html>